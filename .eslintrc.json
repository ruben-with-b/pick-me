{
    "env": {
        "commonjs": true,
        "node": true,
        "jest": true
    },
    "extends": [
        "plugin:vue/essential",
        "google"
    ],
    "parserOptions": {
        "sourceType": "script"
    },
    "plugins": [
        "vue"
    ],
    "rules": {
        "strict": ["error", "global"],
        "func-style": [ "error", "declaration", { "allowArrowFunctions": true } ],
        "prefer-arrow-callback": "error",
        "camelcase": ["error", { "properties": "always"}]
    }
}
