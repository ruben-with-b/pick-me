        "commonjs": true,
        "node": true,
        "jest": true
    },
    "extends": [
        "plugin:vue/essential",
        "google"
    ],
    "parserOptions": {
        "sourceType": "script"
    },
    "plugins": [
        "vue"
    ],
    "rules": {
        "strict": ["error", "global"]
    }
}
